commonLoging/commonLoging=org.apache.commons.logging.Log\=org.apache.commons.logging.impl.Log4JLogger
eclipse.preferences.version=1
isNewProject/isNewProject=true
log4jContent/log4jContent=<?xml version\="1.0" encoding\="UTF-8"?>\r\n<\!-- Talend 8.1 recommended basic log4j2 settings -->\r\n<Configuration \r\n\tstrict\="true"\r\n\tstatus\="warn"\r\n\tschema\="Log4J-V2.0.xsd"\r\n\txmlns\:xsi\="http\://www.w3.org/2001/XMLSchema-instance"\r\n\txmlns\="http\://logging.apache.org/log4j/2.0/config"\r\n\txsi\:schemaLocation\="log4j-config.xsd">\r\n\r\n    <Properties>\r\n        <\!-- Property name\="LOG_DIR">$${sys\:talend_temp\:-$${env\:TALEND_TEMP\:-$${env\:TEMP\:-$${env\:TMP\:-/tmp/talend}}}}/logs/$${env\:TALEND_PROJECT_NAME\:-talend}</Property -->\r\n        <Property name\="LOG_DIR">C\:/tmp/talend/logs/talend</Property>\r\n        <Property name\="LOG_FILE">$${env\:talend.job.name\:-talend_job_name_default}</Property>\r\n        <\!-- Property name\="JOB_LAYOUT_PATTERN">[%-5level] %d{HH\:mm\:ss} %logger{36} \: %msg \: %X \: %x %n</Property -->\r\n        <Property name\="JOB_LAYOUT_PATTERN">[%-5level] %d{HH\:mm\:ss} %logger{36} \: %msg%n</Property>\r\n    </Properties>\r\n\r\n    <Appenders>  \r\n\r\n        <Console name\="CONSOLE" target\="SYSTEM_ERR">  \r\n            <PatternLayout pattern\="${JOB_LAYOUT_PATTERN}" />\r\n        </Console>\r\n\r\n        <Console name\="JOB" target\="SYSTEM_OUT">\r\n            <Filter type\="ThresholdFilter" level\="WARN"/>\r\n            <PatternLayout pattern\="${JOB_LAYOUT_PATTERN}" />\r\n        </Console>\r\n\r\n        <\!-- size based rolling log file appender -->\r\n        <RollingFile name\="sizeRollingFile" fileName\="${LOG_DIR}/${LOG_FILE}.log" filePattern\="${LOG_DIR}/${LOG_FILE}.%i.log" ignoreExceptions\="false">\r\n            <PatternLayout>\r\n                 <Pattern>${JOB_LAYOUT_PATTERN}</Pattern>\r\n            </PatternLayout>\r\n            <Policies>\r\n                <SizeBasedTriggeringPolicy size\="10MB" />\r\n            </Policies>\r\n            <DefaultRolloverStrategy max\="5">\r\n                <Delete basePath\="${LOG_DIR}" maxDepth\="2">\r\n                    <IfFileName glob\="*/app-*.log" />\r\n                    <IfLastModified age\="P30D" />\r\n                </Delete>\r\n            </DefaultRolloverStrategy>\r\n        </RollingFile>\r\n\r\n        <\!-- time based rolling log file (hourly) -->\r\n        <RollingFile name\="hourlyRollingFile" fileName\="${LOG_DIR}/${LOG_FILE}.log" filePattern\="${LOG_DIR}/${LOG_FILE}.%d{yyyy-MM-dd-HH}.log" ignoreExceptions\="false">\r\n            <PatternLayout>\r\n                <Pattern>${JOB_LAYOUT_PATTERN}</Pattern>\r\n            </PatternLayout>\r\n            <Policies>\r\n                <TimeBasedTriggeringPolicy interval\="1" />\r\n            </Policies>\r\n            <DefaultRolloverStrategy max\="5" />\r\n        </RollingFile>\r\n\r\n        <\!-- time based rolling log file (day) -->\r\n        <RollingFile name\="dayRollingFile" fileName\="${LOG_DIR}/${LOG_FILE}.log" filePattern\="${LOG_DIR}/${LOG_FILE}.%d{yyyy-MM-dd}.log" ignoreExceptions\="false">\r\n            <PatternLayout>\r\n                <Pattern>${JOB_LAYOUT_PATTERN}</Pattern>\r\n            </PatternLayout>\r\n            <Policies>\r\n                <TimeBasedTriggeringPolicy interval\="1"/>\r\n            </Policies>\r\n            <DefaultRolloverStrategy max\="5" />\r\n        </RollingFile>\r\n\r\n        <\!-- size and data rolling log file  -->\r\n        <RollingFile name\="sizeDateRollingFile" fileName\="${LOG_DIR}/${LOG_FILE}" filePattern\="${LOG_DIR}/${LOG_FILE}.%d{yyyy-MM-dd-HH}.%i.log" ignoreExceptions\="false">\r\n            <PatternLayout>\r\n                <Pattern>${JOB_LAYOUT_PATTERN}</Pattern>\r\n            </PatternLayout>\r\n            <Policies>\r\n                <OnStartupTriggeringPolicy />\r\n                <SizeBasedTriggeringPolicy size\="10 MB" />\r\n                <TimeBasedTriggeringPolicy interval\="1"/>\r\n            </Policies>\r\n            <DefaultRolloverStrategy max\="5" />\r\n        </RollingFile>\r\n\r\n    </Appenders>  \r\n\r\n\r\n    <Loggers>\r\n        <logger name\="studio_serverless" level\="INFO" additivity\="false">\r\n            <appenderRef ref\="JOB"/>\r\n            <appenderRef ref\="sizeDateRollingFile"/>\r\n        </logger>\r\n\r\n        <Root level\="INFO">\r\n            <AppenderRef ref\="CONSOLE" />  \r\n            <appenderRef ref\="sizeDateRollingFile"/>\r\n        </Root>  \r\n    </Loggers>  \r\n</Configuration>
log4jEnabled/log4jEnabled=true
log4jVersion2/log4jVersion2=true
